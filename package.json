{
  "name": "kypi",
  "version": "0.0.0",
  "packageManager": "bun@^1.2.8",
  "description": "Utility functions for building API clients.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/lucas-labs/kypi#readme",
  "bugs": {
    "url": "https://github.com/lucas-labs/kypi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucas-labs/kypi.git"
  },
  "author": "Lucas Colombo <lucas@lucode.dev>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "bun run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "playground": "vite --config playground/vite.config.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && bun publish",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "ky": "^1.8.1"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.0.1",
    "@sxzz/prettier-config": "^2.2.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.4",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.4",
    "bumpp": "^10.1.0",
    "eslint": "^9.26.0",
    "happy-dom": "^18.0.1",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsdown": "^0.12.9",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.1.3"
  },
  "prettier": "@sxzz/prettier-config"
}
