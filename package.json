{
  "name": "kypi",
  "version": "0.4.2",
  "description": "Type-safe, ergonomic API client builder for TypeScript & React based on ky.",
  "type": "module",
  "keywords": [
    "fetch",
    "api-client",
    "ky",
    "request",
    "requests",
    "http",
    "https",
    "fetching",
    "get",
    "url",
    "curl",
    "wget",
    "net",
    "network",
    "ajax",
    "api",
    "rest",
    "xhr",
    "browser",
    "got",
    "axios",
    "node-fetch"
  ],
  "license": "MIT",
  "homepage": "https://github.com/lucas-labs/kypi#readme",
  "bugs": {
    "url": "https://github.com/lucas-labs/kypi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucas-labs/kypi.git"
  },
  "author": "Lucas Colombo <lucas@lucode.dev>",
  "files": [
    "dist",
    "readme.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "bun run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "playground": "vite --config playground/vite.config.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "ky": "^1.13.0"
  },
  "devDependencies": {
    "@codecov/rollup-plugin": "^1.9.1",
    "@sxzz/eslint-config": "^7.2.7",
    "@sxzz/prettier-config": "^2.2.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "bumpp": "^10.3.1",
    "eslint": "^9.38.0",
    "happy-dom": "^20.0.8",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsdown": "^0.15.9",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.2.4"
  },
  "prettier": "@sxzz/prettier-config"
}
